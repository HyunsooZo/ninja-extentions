{
  "name": "json-to-object",
  "displayName": "Json to Objects (9 languages)",
  "description": "Convert JSON to type definitions for Python, Rust, C, C++, Go, JavaScript, TypeScript, Java, and Kotlin",
  "version": "1.0.1",
  "publisher": "HyunsooJo",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/HyunsooZo/ninja-extentions",
    "directory": "json-to-object"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "json-to-object.convertJson",
        "title": "Convert JSON to Object"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "json-to-object.convertJson",
          "group": "navigation",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "16.x",
    "eslint": "^8.41.0"
  },
  "keywords": [
    "json",
    "converter",
    "typescript",
    "python",
    "rust",
    "java",
    "kotlin",
    "code generator"
  ],
  "author": "HyunsooJo",
  "license": "MIT"
}